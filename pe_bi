#ifndef __PE_BI_H__
#define __PE_BI_H__

#include "pe_base"

// Predefined trait
struct bi_trait30 {
  static const int bit_count = 30;
  static const unsigned int bit_value = 1u << 30;
  static const unsigned int bit_mod_mask = bit_value - 1;
  static const unsigned int output_mod = 100000000;
  static const unsigned int output_mod_dig = 8;
};

struct bi_trait8 {
  static const int bit_count = 8;
  static const unsigned int bit_value = 1u << 8;
  static const unsigned int bit_mod_mask = bit_value - 1;
  static const unsigned int output_mod = 100000000;
  static const unsigned int output_mod_dig = 8;
};

// Predefined allocator
struct bi_allocator_std {
  static unsigned int* allocate(int& length) {
    length = (length + 3 >> 2) << 2;
    return new unsigned int[length];
  }
  static void deallocate(unsigned int* data) { delete[] data; }
};

template <int SIZE>
struct bi_allocator_fixed_size_impl {
  vector<unsigned int*> available;
  int top;

  bi_allocator_fixed_size_impl() { top = 0; }

  unsigned int* allocate(int& length) {
    if (length > SIZE) {
      dbg(length);
    }
    PE_ASSERT(length <= SIZE);
    length = SIZE;
    if (top == available.size()) {
      available.push_back(new unsigned int[SIZE]);
    }
    return available[top++];
  }

  void deallocate(unsigned int* ptr) { available[--top] = ptr; }
};

static bi_allocator_fixed_size_impl<1 << 16> allocator_16;
static bi_allocator_fixed_size_impl<1 << 20> allocator_20;

auto& allocator_impl = allocator_16;

struct bi_allocator_fixed_size {
  static unsigned int* allocate(int& length) {
    return allocator_impl.allocate(length);
  }
  static void deallocate(unsigned int* data) {
    allocator_impl.deallocate(data);
  }
};

// Configuration of BigInteger.
typedef bi_trait30 bi_trait;
typedef bi_allocator_std bi_allocator;

class BigInteger {
 public:
  // constructors, destructors, value semantics.
  BigInteger() : BigInteger(zero_initialize_tag) {
    reserve(1);
    data_[0] = 0;
    sign_ = 0;
  }

  ~BigInteger() {
    if (data_) {
      bi_allocator::deallocate(data_);
    }
  }

  BigInteger(const BigInteger& other) : BigInteger(zero_initialize_tag) {
    reserve(other.bufferLength_);
    sign_ = other.sign_;
    pos_ = other.pos_;
    copy(other.data_, other.data_ + other.pos_ + 1, data_);
  }

  BigInteger(BigInteger&& other) : BigInteger(zero_initialize_tag) {
    std::swap(data_, other.data_);
    std::swap(bufferLength_, other.bufferLength_);
    std::swap(sign_, other.sign_);
    std::swap(pos_, other.pos_);
  }

  BigInteger& operator=(const BigInteger& other) {
    return *this = BigInteger(other);
  }

  BigInteger& operator=(BigInteger&& other) {
    std::swap(data_, other.data_);
    std::swap(bufferLength_, other.bufferLength_);
    std::swap(sign_, other.sign_);
    std::swap(pos_, other.pos_);
    return *this;
  }

  template <typename T>
  BigInteger(T value) : BigInteger(zero_initialize_tag) {
    if (value == 0) {
      reserve(1);
      data_[0] = 0;
      sign_ = 0;
      return;
    }

    int need = 0;
    typedef typename std::make_unsigned<T>::type unsingedT;
    unsingedT absValue = value > 0 ? value : -value;

    if (absValue < bi_trait::bit_value) {
      reserve(1);
      data_[0] = absValue;
    } else {
      for (auto t = absValue; t > 0; ++need, t >>= bi_trait::bit_count)
        ;
      reserve(need);

      pos_ = -1;
      for (auto t = absValue; t > 0; data_[++pos_] = t & bi_trait::bit_mod_mask,
                t >>= bi_trait::bit_count)
        ;
    }
    sign_ = value > 0 ? 1 : -1;
  }

  template <typename T>
  BigInteger& operator=(T value) {
    return *this = BigInteger(value);
  }

  BigInteger(const string& str) : BigInteger(zero_initialize_tag) {
    const int n = str.size();
    if (n == 0) {
      reserve(1);
      data_[0] = 0;
      sign_ = 0;
      return;
    }
    std::vector<unsigned int> value(n);
    for (int i = 0, j = n - 1; i < n; ++i, --j) {
      value[j] = str[i] - '0';
    }

    int now = n - 1;
    while (now > 0 && value[now] == 0) --now;

    std::vector<unsigned int> result;
    do {
      uint64 add = 0;
      for (int i = now; i > 0; --i) {
        const uint64 x = add + value[i];
        const uint64 next_add = (x & bi_trait::bit_mod_mask) * 10;
        value[i] = x >> bi_trait::bit_count;
        add = next_add;
      }
      uint64 x = add + value[0];
      value[0] = x >> bi_trait::bit_count;
      result.push_back(x & bi_trait::bit_mod_mask);
      while (now > 0 && value[now] == 0) --now;
    } while (now > 0 || value[now] > 0);

    reserve(result.size());

    sign_ = 1;
    pos_ = max(0, (int)result.size() - 1);
    copy(result.begin(), result.end(), data_);
    fixPos();
  }

  template <typename T>
  BigInteger(const vector<T>& v) : BigInteger(zero_initialize_tag) {
    if (v.empty()) {
      reserve(1);
      data_[0] = 0;
      sign_ = 0;
      return;
    }

    const int n = (int)v.size();
    int need = (n + bi_trait::bit_count - 1) / bi_trait::bit_count;
    reserve(need);

    unsigned int value = 0;
    unsigned int flag = 1;
    for (int i = 0; i < n; ++i) {
      if (v[i]) {
        value |= flag;
      }
      flag <<= 1;
      if ((i + 1) % bi_trait::bit_count == 0) {
        data_[pos_++] = value;
        value = 0;
        flag = 1;
      }
    }
    if (n % bi_trait::bit_count) {
      data_[pos_++] = value;
    }
    --pos_;
    sign_ = 1;
    fixPos();
  }

 private:
  // Internal constructor
  static const struct alloc_mem {
  } alloc_mem_tag;
  
  static const struct zero_initialize {
  } zero_initialize_tag;

  BigInteger(int size, alloc_mem) : BigInteger(zero_initialize_tag) {
    reserve(size);
    data_[0] = 0;
    sign_ = 0;
  }

  BigInteger(const BigInteger& other, int minBuffer) : BigInteger(zero_initialize_tag) {
    reserve(max(other.bufferLength_, minBuffer));
    sign_ = other.sign_;
    pos_ = other.pos_;
    copy(other.data_, other.data_ + other.pos_ + 1, data_);
  }
  
  BigInteger(zero_initialize)
      : data_(nullptr), bufferLength_(0), sign_(0), pos_(0) {
  }

 public:
  // compare operators
  friend inline int operator>(const BigInteger& l, const BigInteger& r);
  friend inline int operator<(const BigInteger& l, const BigInteger& r);
  friend inline int operator==(const BigInteger& l, const BigInteger& r);
  friend inline int operator>=(const BigInteger& l, const BigInteger& r);
  friend inline int operator<=(const BigInteger& l, const BigInteger& r);

  // binary operators
  friend inline BigInteger operator+(const BigInteger& l, const BigInteger& r);
  friend inline BigInteger operator-(const BigInteger& l, const BigInteger& r);

  friend inline BigInteger operator*(const BigInteger& l, const BigInteger& r);
  template <typename T>
  friend inline BigInteger operator*(const BigInteger& l, T r);
  template <typename T>
  friend inline BigInteger operator*(T l, const BigInteger& r);

  friend inline BigInteger operator/(const BigInteger& l, const BigInteger& r);
  template <typename T>
  friend inline BigInteger operator/(const BigInteger& l, T r);
  friend inline tuple<BigInteger, BigInteger> div(const BigInteger& l,
                                                  const BigInteger& r);
  template <typename T>
  friend inline tuple<BigInteger, T> div(const BigInteger& l, T r);
  template <typename T>
  friend inline void divInplace(BigInteger& l, T r, T& remain);

  friend inline BigInteger operator%(const BigInteger& l, const BigInteger& r);
  template <typename T>
  friend inline T operator%(const BigInteger& l, T r);

  friend inline BigInteger operator&(const BigInteger& l, const BigInteger& r);
  friend inline BigInteger operator^(const BigInteger& l, const BigInteger& r);
  friend inline BigInteger operator|(const BigInteger& l, const BigInteger& r);

  template <typename T>
  friend inline BigInteger operator<<(const BigInteger& l, T n);

  template <typename T>
  friend inline BigInteger operator>>(const BigInteger& l, T n);

 public:
  // unary operations
  BigInteger operator-() const {
    BigInteger ret(*this);
    ret.sign_ = -ret.sign_;
    return ret;
  }

  const BigInteger& operator+() const { return *this; }

  BigInteger operator~() const {
    BigInteger ret(*this);
    for (int i = 0; i < ret.pos_; ++i) ret[i] ^= bi_trait::bit_mod_mask;
    for (unsigned int x = 1, y = ret[ret.pos_]; x <= y;
         ret[ret.pos_] ^= x, x <<= 1)
      ;

    ret.fixPos();

    return ret;
  }

 private:
  static int absCompare(const BigInteger& l, const BigInteger& r) {
    int posL = l.pos_;
    int posR = r.pos_;

    if (posL != posR) {
      return posL > posR ? 1 : -1;
    }

    for (; posL >= 0 && l[posL] == r[posL]; --posL)
      ;

    return posL >= 0 ? (l[posL] > r[posL] ? 1 : -1) : 0;
  }

  static BigInteger absAdd(const BigInteger& l, const BigInteger& r) {
    BigInteger ret(l, max(l.size(), r.size()) + 1);
    absAddInplace(ret, r);
    return ret;
  }

  static void absAddInplace(BigInteger& l, const BigInteger& r) {
    l.reserve(l.size() + r.size() + 1);

    int maxPos = max(l.pos_, r.pos_);
    int minPos = min(l.pos_, r.pos_);

    int idx = 0;
    unsigned int inc = 0;

    while (idx <= minPos) {
      unsigned int t = l[idx] + r[idx] + inc;
      l[idx++] = t & bi_trait::bit_mod_mask;
      inc = t >> bi_trait::bit_count;
    }

    const auto* largeData = l.pos_ > r.pos_ ? l.data_ : r.data_;
    while (idx <= maxPos) {
      unsigned int t = largeData[idx] + inc;
      l[idx++] = t & bi_trait::bit_mod_mask;
      inc = t >> bi_trait::bit_count;
    }

    if (inc) {
      l[idx++] = inc;
    }

    l.pos_ = max(idx - 1, 0);
    l.fixPos();
  }

  static BigInteger absSub(const BigInteger& l, const BigInteger& r) {
    BigInteger ret(l);
    absSubInplace(ret, r);
    return ret;
  }

  static void absSubInplace(BigInteger& l, const BigInteger& r) {
    PE_ASSERT(BigInteger::absCompare(l, r) >= 0);

    int maxPos = l.pos_;
    int minPos = r.pos_;

    int borrow = 0;
    for (int curr = 0; curr <= minPos; ++curr) {
      int nextBorrow = 0;
      unsigned int t = l[curr];
      while (t < r[curr] + borrow) {
        nextBorrow++;
        t += bi_trait::bit_value;
      }
      l[curr] = t - r[curr] - borrow;
      borrow = nextBorrow;
    }
    for (int curr = minPos + 1; curr <= maxPos; ++curr) {
      int nextBorrow = 0;
      unsigned int t = l[curr];
      while (t < borrow) {
        nextBorrow++;
        t += bi_trait::bit_value;
      }
      l[curr] = t - borrow;
      borrow = nextBorrow;
    }
    l.fixPos();
  }

  static BigInteger absMul(const BigInteger& l, const BigInteger& r) {
    int posL = l.pos_;
    int posR = r.pos_;

    const int newSize = l.size() + r.size() + 1;

    BigInteger ret(newSize, alloc_mem_tag);
    fill(ret.data_, ret.data_ + newSize, 0);

    for (int i = 0; i <= posR; ++i) {
      auto t = r[i];
      uint64 inc = 0;
      int j = 0;
      for (; j <= posL; ++j) {
        inc += (uint64)t * l[j] + ret[i + j];
        ret[i + j] = inc & bi_trait::bit_mod_mask;
        inc >>= bi_trait::bit_count;
      }
      for (; inc; inc >>= bi_trait::bit_count) {
        ret[i + j++] = inc & bi_trait::bit_mod_mask;
      }
    }
    ret.pos_ = posL + posR + 2;
    ret.fixPos();
    return ret;
  }

  template <typename T>
  static BigInteger absMul(const BigInteger& l, T r) {
    BigInteger ret(l, l.size() + BigInteger(r).size() + 1);
    absMulInplace(ret, r);
    return ret;
  }

  template <typename T>
  static void absMulInplace(BigInteger& l, T r) {
    l.reserve(l.size() + BigInteger(r).size() + 1);

    uint64 inc = 0;
    int top = 0;
    for (int i = 0; i <= l.pos_; ++i) {
      uint64 v = (uint64)l[i] * r + inc;
      l[top++] = v & bi_trait::bit_mod_mask;
      inc = v >> bi_trait::bit_count;
    }

    while (inc > 0) {
      l[top++] = inc & bi_trait::bit_mod_mask;
      inc >>= bi_trait::bit_count;
    }

    l.pos_ = top - 1;
    l.fixPos();
  }

  static BigInteger absDiv(const BigInteger& l, const BigInteger& r,
                           BigInteger& remain) {
    int t = absCompare(l, r);
    if (t < 0) {
      remain = l;
      return 0;
    }
    if (t == 0) {
      remain = 0;
      return 1;
    }

    vector<unsigned int> rb;
    vector<unsigned int> rr = absDiv<unsigned int>(
        l.toBinVector<unsigned int>(), r.toBinVector<unsigned int>(), rb);
    remain = BigInteger(rb);
    return rr;
  }

  static BigInteger absDiv(const BigInteger& l, const BigInteger& r) {
    int t = absCompare(l, r);
    if (t < 0) {
      return 0;
    }
    if (t == 0) {
      return 1;
    }

    vector<unsigned int> rb;
    vector<unsigned int> rr = absDiv<unsigned int>(
        l.toBinVector<unsigned int>(), r.toBinVector<unsigned int>(), rb);
    return rr;
  }

  template <typename T>
  static vector<T> absDiv(const vector<T>& l, const vector<T>& r,
                          vector<T>& remain) {
    vector<T> result;
    remain = l;

    int divR = r.size();
    int pos = (int)l.size() - 1;
    int end = pos - divR + 1;
    for (; end >= 0;) {
      if (pos - end + 1 < divR) {
        result.push_back(0);
        --end;
        continue;
      }
      if (remain[pos] == 0) {
        --pos;
        continue;
      }
      int gt = 1;
      if (pos - end + 1 == divR) {
        for (int i = pos, j = divR - 1; i >= end; --i, --j) {
          if (remain[i] < r[j]) {
            gt = 0;
            break;
          } else if (remain[i] > r[j]) {
            break;
          }
        }
      }
      result.push_back(gt);
      if (gt == 0) {
        --end;
        continue;
      }

      int borrow = 0;
      int i = end, j = 0;
      for (; j < divR; ++i, ++j) {
        int nextBorrow = 0;
        int t = remain[i];
        while (t < r[j] + borrow) {
          t += 2;
          ++nextBorrow;
        }
        remain[i] = t - r[j] - borrow;
        borrow = nextBorrow;
      }

      for (; i <= pos; ++i) {
        int nextBorrow = 0;
        int t = remain[i];
        while (t < borrow) {
          t += 2;
          ++nextBorrow;
        }
        remain[i] = t - borrow;
        borrow = nextBorrow;
      }
      --end;
    }

    if (result.size() == 0) result.push_back(0);
    reverse(result.begin(), result.end());
    fixSize(result);
    fixSize(remain);
    return result;
  }

  template <typename T>
  static BigInteger absDiv(const BigInteger& l, T r, T& remain) {
    BigInteger t(l);
    absDivInplace(t, r, remain);
    return t;
  }

  template <typename T>
  static void absDivInplace(BigInteger& l, T r, T& remain) {
    l.fixPos();

    uint64 add = 0;
    for (int i = l.pos_; i > 0; --i) {
      const uint64 x = add + l.data_[i];
      const uint64 next_add = (x % r) << bi_trait::bit_count;
      l.data_[i] = x / r;
      add = next_add;
    }
    uint64 x = add + l.data_[0];
    l.data_[0] = x / r;
    remain = x % r;
    l.fixPos();
  }

 public:
  BigInteger& operator+=(const BigInteger& other) {
    if (other.isZero()) {
      // Do nothing
    } else if (isZero()) {
      *this = other;
    } else if (getSign() == other.getSign()) {
      absAddInplace(*this, other);
    } else {
      int c = absCompare(*this, other);
      if (c == 0) {
        pos_ = 0;
        data_[0] = 0;
        sign_ = 0;
      } else if (c > 0) {
        absSubInplace(*this, other);
      } else {
        BigInteger t(other);
        absSubInplace(t, *this);
        *this = std::move(t);
      }
    }
    return *this;
  }

  BigInteger& operator-=(const BigInteger& other) {
    if (other.isZero()) {
      // Do nothing
    } else if (isZero()) {
      *this = other;
      sign_ = -other.sign_;
    } else if (getSign() != other.getSign()) {
      absAddInplace(*this, other);
    } else {
      int c = absCompare(*this, other);
      if (c == 0) {
        pos_ = 0;
        data_[0] = 0;
        sign_ = 0;
      } else if (c > 0) {
        absSubInplace(*this, other);
      } else {
        BigInteger t(other);
        absSubInplace(t, *this);
        *this = std::move(t);
        sign_ = -other.sign_;
      }
    }
    return *this;
  }

  BigInteger& operator*=(const BigInteger& other) {
    *this = *this * other;
    return *this;
  }

  template <typename T>
  BigInteger& operator*=(T other) {
    if (isZero()) {
      return *this;
    }
    if (other == 0) {
      pos_ = 0;
      data_[0] = 0;
      sign_ = 0;
      return *this;
    }
    absMulInplace(*this, other > 0 ? other : -other);
    sign_ = sign_ * (other > 0 ? 1 : -1);
    return *this;
  }

  BigInteger& operator/=(const BigInteger& other) {
    *this = *this / other;
    return *this;
  }

  template <typename T>
  BigInteger& operator/=(T other) {
    T r;
    divInplace(*this, other, &r);
    return *this;
  }

  BigInteger& operator%=(const BigInteger& other) {
    *this = *this % other;
    return *this;
  }

  template <typename T>
  BigInteger& operator%=(T other) {
    T r;
    divInplace(*this, other, &r);
    return *this = r;
  }

  BigInteger& operator^=(const BigInteger& other) {
    *this = *this ^ other;
    return *this;
  }

  BigInteger& operator|=(const BigInteger& other) {
    *this = *this | other;
    return *this;
  }

  template <typename T>
  BigInteger& operator>>=(T other) {
    *this = *this >> other;
    return *this;
  }

  template <typename T>
  BigInteger& operator<<=(T other) {
    *this = *this << other;
    return *this;
  }

  BigInteger& operator++() {
    *this += 1;
    return *this;
  }

  BigInteger operator++(int) {
    BigInteger ret(*this);
    *this += 1;
    return ret;
  }

  BigInteger& operator--() {
    *this -= 1;
    return *this;
  }

  BigInteger operator--(int) {
    BigInteger ret(*this);
    *this -= 1;
    return ret;
  }

 private:
  void fixPos() const {
    while (pos_ > 0 && data_[pos_] == 0) --pos_;
    if (pos_ == 0 && data_[0] == 0) sign_ = 0;
  }

  template <typename T>
  static void fixSize(vector<T>& v) {
    if (v.empty()) v.push_back(0);
    int now = (int)v.size() - 1;
    while (now > 0 && v[now] == 0) --now;
    v.resize(now + 1);
  }

  void reserve(int newSize) {
    PE_ASSERT(newSize > 0);
    unsigned int* oldData = nullptr;
    if (data_) {
      if (newSize > bufferLength_) {
        oldData = data_;
      } else {
        return;
      }
    }

    data_ = bi_allocator::allocate(newSize);
    if (oldData != nullptr) {
      // copy used data only.
      copy(oldData, oldData + pos_ + 1, data_);
      bi_allocator::deallocate(oldData);
    }
    bufferLength_ = newSize;
  }

 public:
  // general util method
  friend ostream& operator<<(ostream& o, const BigInteger& bi);

  string toString() const {
    BigInteger t(*this);
    std::vector<unsigned int> mods;
    t.fixPos();
    string ret = t.sign_ == -1 ? "-" : "";
    do {
      uint64 add = 0;
      for (int i = t.pos_; i > 0; --i) {
        const uint64 x = add + t.data_[i];
        const uint64 next_add = (x % bi_trait::output_mod)
                                << bi_trait::bit_count;
        t.data_[i] = x / bi_trait::output_mod;
        add = next_add;
      }
      uint64 x = add + t.data_[0];
      t.data_[0] = x / bi_trait::output_mod;
      mods.push_back(x % bi_trait::output_mod);
      t.fixPos();
    } while (!t.isZero());

    int idx = (int)mods.size() - 1;
    char buff[32];
    sprintf(buff, "%u", mods[idx--]);
    ret += buff;
    for (; idx >= 0; --idx) {
      sprintf(buff, "%0*u", bi_trait::output_mod_dig, mods[idx]);
      ret += buff;
    }
    return ret;
  }

  void dump() const {
    dbg(sign_);
    dbg(pos_);
    dbg(bufferLength_);
    for (int i = 0; i <= pos_; ++i) cerr << data_[i] << ",";
    cerr << endl;
  }

  bool isZero() const { return sign_ == 0; }

  int getSign() const { return sign_; }

  unsigned int operator[](int idx) const { return data_[idx]; }

  unsigned int& operator[](int idx) { return data_[idx]; }

  const unsigned int* data() const { return data_; }

  unsigned int* data() { return data_; }

  int size() const { return pos_ + 1; }

  template <typename T>
  vector<T> toBinVector() const {
    vector<T> ret;
    for (int i = 0; i <= pos_; ++i) {
      auto t = data_[i];
      for (int j = 0; j < bi_trait::bit_count; ++j)
        ret.push_back(t & 1), t >>= 1;
    }
    fixSize(ret);
    return ret;
  }

  template <typename T>
  T toInt() const {
    T result = 0;
    for (int i = pos_; i >= 0; --i)
      result = (result << bi_trait::bit_count) | data_[i];
    return result;
  }

  int bitCount() const {
    int ret = pos_ * bi_trait::bit_count;
    int hiCnt = 0;
    for (unsigned int x = 1; x <= data_[pos_]; ++hiCnt, x <<= 1)
      ;
    return ret + hiCnt;
  }

  int bitAt(int t) const {
    const int x = t / bi_trait::bit_count;
    const int y = t % bi_trait::bit_count;
    return x > pos_ ? 0 : (data_[x] >> y) & 1;
  }

 private:
  unsigned int* data_;
  int bufferLength_;
  mutable int pos_;
  mutable int sign_;
};

inline int operator>(const BigInteger& l, const BigInteger& r) {
  const int s1 = l.sign_;
  const int s2 = r.sign_;

  if (s1 < 0) {
    return s2 < 0 && BigInteger::absCompare(l, r) < 0;
  } else if (s1 > 0) {
    return s2 <= 0 || BigInteger::absCompare(l, r) > 0;
  } else {
    return s2 < 0;
  }
}

inline int operator<(const BigInteger& l, const BigInteger& r) {
  const int s1 = l.sign_;
  const int s2 = r.sign_;

  if (s1 < 0) {
    return s2 >= 0 || BigInteger::absCompare(l, r) > 0;
  } else if (s1 > 0) {
    return s2 > 0 && BigInteger::absCompare(l, r) < 0;
  } else {
    return s2 > 0;
  }
}

inline int operator==(const BigInteger& l, const BigInteger& r) {
  return l.sign_ == r.sign_ && BigInteger::absCompare(l, r) == 0;
}

inline int operator>=(const BigInteger& l, const BigInteger& r) {
  const int s1 = l.sign_;
  const int s2 = r.sign_;

  if (s1 < 0) {
    return s2 < 0 && BigInteger::absCompare(l, r) <= 0;
  } else if (s1 > 0) {
    return s2 <= 0 || BigInteger::absCompare(l, r) >= 0;
  } else {
    return s2 <= 0;
  }
}

inline int operator<=(const BigInteger& l, const BigInteger& r) {
  const int s1 = l.sign_;
  const int s2 = r.sign_;

  if (s1 < 0) {
    return s2 >= 0 || BigInteger::absCompare(l, r) >= 0;
  } else if (s1 > 0) {
    return s2 > 0 && BigInteger::absCompare(l, r) <= 0;
  } else {
    return s2 >= 0;
  }
}

inline BigInteger operator+(const BigInteger& l, const BigInteger& r) {
  BigInteger ret(l);
  ret += r;
  return ret;
}

inline BigInteger operator-(const BigInteger& l, const BigInteger& r) {
  if (r.isZero()) {
    return l;
  } else if (l.isZero()) {
    return -r;
  } else if (l.getSign() != r.getSign()) {
    BigInteger t(l);
    BigInteger::absAddInplace(t, r);
    return t;
  } else {
    int c = BigInteger::absCompare(l, r);
    if (c == 0) {
      return 0;
    } else if (c > 0) {
      BigInteger t(l);
      BigInteger::absSubInplace(t, r);
      return t;
    } else {
      BigInteger t(r);
      BigInteger::absSubInplace(t, l);
      t.sign_ = -r.sign_;
      return t;
    }
  }
}

inline BigInteger operator*(const BigInteger& l, const BigInteger& r) {
  if (l.isZero() || r.isZero()) return 0;

  const int s = l.getSign() * (r > 0 ? 1 : -1);
  BigInteger ret(BigInteger::absMul(l, r));
  ret.sign_ = s;

  return ret;
}

template <typename T>
inline BigInteger operator*(const BigInteger& l, T r) {
  if (r == 0 || l.isZero()) return 0;

  const int s = l.getSign() * (r > 0 ? 1 : -1);
  BigInteger ret(BigInteger::absMul(l, r > 0 ? r : -r));
  ret.sign_ = s;

  return ret;
}

template <typename T>
inline BigInteger operator*(T l, const BigInteger& r) {
  return r * l;
}

inline BigInteger operator/(const BigInteger& l, const BigInteger& r) {
  BigInteger u, v;
  tie(u, v) = div(l, r);
  return u;
}

template <typename T>
inline BigInteger operator/(const BigInteger& l, T r) {
  BigInteger u;
  T v;
  tie(u, v) = div(l, r);
  return u;
}

inline tuple<BigInteger, BigInteger> div(const BigInteger& l,
                                         const BigInteger& r) {
  if (l.isZero() && r.isZero()) {
    return make_tuple(1, 0);
  }
  if (l.isZero() || r.isZero()) {
    return make_tuple(0, 0);
  }

  BigInteger x;
  BigInteger ret(BigInteger::absDiv(l, r, x));
  ret.sign_ = l.sign_ * r.sign_;
  ret.fixPos();
  x.sign_ = l.sign_;
  x.fixPos();

  return make_tuple(std::move(ret), std::move(x));
}

template <typename T>
inline tuple<BigInteger, T> div(const BigInteger& l, T r) {
  if (l.isZero() && r == 0) {
    return make_tuple(1, 0);
  }
  if (l.isZero() || r == 0) {
    return make_tuple(0, 0);
  }

  int signl = l.sign_;
  T x;
  BigInteger ret(l);
  BigInteger::absDivInplace(ret, r, x);
  ret.sign_ = signl * (r > 0 ? 1 : -1);
  ret.fixPos();
  if (l.sign_ < 0) {
    PE_ASSERT(x == 0 || !std::is_unsigned<T>::value);
    x = -x;
  }

  return make_tuple(std::move(ret), x);
}

template <typename T>
inline void divInplace(BigInteger& l, T r, T& remain) {
  if (l.isZero() && r == 0) {
    l = 1;
    remain = 0;
    return;
  }

  if (l.isZero() || r == 0) {
    if (!l.isZero()) {
      l.pos_ = 0;
      l.data_[0] = 0;
      l.sign_ = 0;
    }
    remain = 0;
    return;
  }

  int signl = l.sign_;
  BigInteger::absDivInplace(l, r, remain);
  l.sign_ = signl * (r > 0 ? 1 : -1);
  l.fixPos();
  if (l.sign_ < 0) {
    PE_ASSERT(remain == 0 || !std::is_unsigned<T>::value);
    remain = -remain;
  }
}

inline BigInteger operator%(const BigInteger& l, const BigInteger& r) {
  BigInteger u, v;
  tie(u, v) = div(l, r);
  return v;
}

template <typename T>
inline T operator%(const BigInteger& l, T r) {
  BigInteger u;
  T v;
  tie(u, v) = div(l, r);
  return v;
}

inline BigInteger operator&(const BigInteger& l, const BigInteger& r) {
  const int minPos = min(l.pos_, r.pos_);
  BigInteger ret(minPos + 1, BigInteger::alloc_mem_tag);
  for (int i = 0; i <= minPos; ++i) {
    ret[i] = l[i] & r[i];
  }
  ret.pos_ = minPos;
  ret.fixPos();
  return ret;
}

inline BigInteger operator^(const BigInteger& l, const BigInteger& r) {
  const int maxPos = max(l.pos_, r.pos_);
  BigInteger ret(maxPos + 1, BigInteger::alloc_mem_tag);
  for (int i = 0; i <= maxPos; ++i) {
    unsigned int t = 0;
    if (i <= l.pos_) t ^= l[i];
    if (i <= r.pos_) t ^= r[i];
    ret[i] = t;
  }
  ret.pos_ = maxPos;
  ret.fixPos();
  return ret;
}

inline BigInteger operator|(const BigInteger& l, const BigInteger& r) {
  const int maxPos = max(l.pos_, r.pos_);
  BigInteger ret(maxPos + 1, BigInteger::alloc_mem_tag);
  for (int i = 0; i <= maxPos; ++i) {
    unsigned int t = 0;
    if (i <= l.pos_) t |= l[i];
    if (i <= r.pos_) t |= r[i];
    ret[i] = t;
  }
  ret.pos_ = maxPos;
  ret.fixPos();
  return ret;
}

template <typename T>
inline BigInteger operator<<(const BigInteger& l, T n) {
  if (l.getSign() == 0) {
    return 0;
  }
  if (n == 0) {
    return l;
  }
  const int bitCnt = l.bitCount();
  vector<unsigned int> v(n + bitCnt, 0);
  for (int i = 0, j = n; i < bitCnt; ++i, ++j) {
    const int x = i / bi_trait::bit_count;
    const int y = i % bi_trait::bit_count;
    v[j] = (l[x] >> y) & 1;
  }
  BigInteger ret(v);
  ret.sign_ = l.sign_;
  ret.fixPos();
  return ret;
}

template <typename T>
inline BigInteger operator>>(const BigInteger& l, T n) {
  if (l.getSign() == 0) {
    return 0;
  }
  if (n == 0) {
    return l;
  }
  const int bitCnt = l.bitCount();
  if (n >= bitCnt) {
    return 0;
  }

  vector<unsigned int> v(bitCnt - n, 0);
  for (int i = n, j = 0; i < bitCnt; ++i, ++j) {
    const int x = i / bi_trait::bit_count;
    const int y = i % bi_trait::bit_count;
    v[j] = (l[x] >> y) & 1;
  }

  BigInteger ret(v);
  ret.sign_ = l.sign_;
  ret.fixPos();

  return ret;
}

template <typename T>
inline BigInteger power(const BigInteger& l, T n) {
  BigInteger ret(1);
  BigInteger x(l);
  for (; n > 0; n >>= 1) {
    if (n & 1) ret = ret * x;
    if (n > 1) x = x * x;
  }
  return ret;
}

template <typename T>
inline BigInteger power_mod(const BigInteger& l, T n, const BigInteger& mod) {
  if (mod == 1) {
    return 0;
  }
  BigInteger ret(1);
  BigInteger x(l % mod);
  for (; n > 0; n >>= 1) {
    if (n & 1) ret = ret * x % mod;
    if (n > 1) x = x * x % mod;
  }
  return ret;
}

inline BigInteger power_mod(const BigInteger& l, const BigInteger& m,
                            const BigInteger& mod) {
  if (mod == 1) {
    return 0;
  }
  BigInteger ret(1);
  BigInteger x(l % mod);
  BigInteger n(m);
  for (; n.getSign() > 0;) {
    if (n[0] & 1) ret = ret * x % mod;
    n >>= 1;
    if (n.getSign() > 0) x = x * x % mod;
  }
  return ret;
}

inline BigInteger exgcd(const BigInteger& a, const BigInteger& b, BigInteger& x,
                        BigInteger& y) {
  if (b == 0) {
    x = 1;
    y = 0;
    return a;
  }

  BigInteger x1 = 0, y1 = 0;
  BigInteger ret = exgcd(b, a % b, x1, y1);
  // b * x1 + (a%b) * y1 = ret
  // b * x1 + (a - a / b * b) * y1 = ret
  // b * x1 + a * y1 - a / b * b * y1 = ret
  // y1 * a + (x1 - a / b * y1) b = ret
  y = x1 - a / b * y1;
  x = std::move(y1);
  return ret;
}

inline BigInteger gcd(BigInteger m, BigInteger n) {
  while (!n.isZero()) {
    BigInteger t = m % n;
    m = n;
    n = std::move(t);
  }
  return m;
}

inline BigInteger mod_inv(const BigInteger& a, const BigInteger& mod) {
  BigInteger x, y;
  exgcd(a, mod, x, y);
  x %= mod;
  if (x.getSign() < 0) {
    x += mod;
  }
  return x;
}

inline BigInteger fabs(const BigInteger& n) {
  return n.getSign() >= 0 ? n : -n;
}

inline BigInteger abs(const BigInteger& n) { return n.getSign() >= 0 ? n : -n; }

ostream& operator<<(ostream& o, const BigInteger& bi) {
  return o << bi.toString();
}

BigInteger operator"" _bi(const char* sv, size_t sc) {
  return BigInteger(string(sv));
}

BigInteger operator"" _bi(unsigned long long v) { return BigInteger(v); }

typedef BigInteger bi;

struct FacModer {
  std::vector<bi> F;
  int64 p;
  int64 e;
  bi mod;
  bi phi;

  FacModer(const int64 p, const int64 e) : p(p), e(e) {
    int64 pe = p * e;
    mod = power(bi(p), e);
    phi = power(bi(p), (e - 1)) * (p - 1);
    F.resize(pe + 1, 1);
    for (int i = 1; i <= pe; ++i) {
      if (i % p == 0)
        F[i] = F[i - 1];
      else
        F[i] = F[i - 1] * i % mod;
    }
  }

  bi cal(const bi& N) {
    bi u;
    bi v;
    tie(u, v) = div(N, p);
    int64 vv = v.toInt<int64>();
    int64 r = e + 1 >> 1;

    bi ret = 1;
    // F(up, p)
    const bi u2 = u * u;

    for (int j = 1; j <= r; ++j) {
      bi beta_numer = u;
      bi beta_denom = j;
      const int j2 = j * j;
      for (int i = 1; i <= r; ++i)
        if (i != j) beta_numer *= u2 - i * i, beta_denom *= j2 - i * i;
      auto beta = beta_numer / beta_denom % phi;
      if (beta.getSign() < 0) beta += phi;
      ret = ret * power_mod(F[j * p], beta, mod) % mod;
    }

    if (p == 2) {
      int64 left_mod4 = (((u * p + 1)[0] & 4) >> 1) + 1;
      int64 right_mod4 = ret[0] & 3;
      if (left_mod4 != right_mod4) {
        ret = (mod - ret) % mod;
      }
    }

    // F(v, p)
    ret = ret * F[vv] % mod;

    // (p + v, v)_p
    for (int j = 1; j < e; ++j) {
      auto base = F[j * p + vv] * mod_inv(F[vv] * F[j * p], mod) % mod;
      bi alpha_numer = u;
      bi alpha_denom = j;
      for (int i = 1; i < e; ++i)
        if (i != j) {
          alpha_numer *= u - i;
          alpha_denom *= j - i;
        }
      auto alpha = alpha_numer / alpha_denom % phi;
      if (alpha.getSign() < 0) alpha += phi;
      ret = ret * power_mod(base, alpha, mod) % mod;
    }

    return ret;
  }
};
#endif
